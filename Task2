//2.Найти произведение пар чисел в одномерном массиве. Парой считаем первый и последний элемент, второй и предпоследний и т.д.
void MultiArr(int[]array)
{

for(int i = 0; i<array.Length/2;i++){

        array[i] = array[i] * array[array.Length-1-i];
        Console.WriteLine($"{array[i]}");

}
}
int[] array = new int[] { 2, 4, 5, 7, 3, 5 };
MultiArr(array);
